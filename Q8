class Main {
    public static void main(String[] args) {

        String[] manufacturer = { "Audi", "BMW", "VW", "Seat" };
        String[] model = { "A4", "320i", "Golf", "Leon" };

        printList(manufacturer, model);
    }

    public static void printList(String[] man, String[] mod) {
        for (int i = 0; i < man.length; i++) {
            System.out.println(man[i] + " " + mod[i]);
            }
        }
    }
