class Main {
  public static void main(String[] args) {
    int [] a;
    a = generateTriNums();
    printArray(a);
    //reference = printArray(arr);
    // 1. Declare an array reference 
    // 2. Call the method generateTriNums() 
    //    and assign the returned array to 
    //    the array reference from 1.
    // 3. Call the method printArray() to 
    //    display the contents of the array  
    
  }

  public static int[] generateTriNums(){  
    int[] arr = {1,3,6,10,15};  
    return(arr); // Return reference of arr  
  }  

  public static void printArray(int[] arr){  
    for(int i: arr){ //For-Each loop
      System.out.println(i);
    }
  } 
}
